{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: arm_controller",
            "type": "debugpy",
            "request": "launch",
            "module": "tidybot.server.arm_server.arm_controller",
            "cwd": "${workspaceFolder}/.."
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: arm_server",
            "type": "debugpy",
            "request": "launch",
            "module": "tidybot.server.arm_server.arm_server",
            "cwd": "${workspaceFolder}/.."
        },
    ]
}